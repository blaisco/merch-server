<!DOCTYPE html>
<html>
<head>
  <title><%= @title ? (@title + " | " + "Video Game Merch") : "Video Game Merch" %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container_12">
    <div class="grid_12">
    
      <div id="message_center">
        <%= raw "<div class='notice'>#{notice}</div>" if notice %>
        <%= raw "<div class='notice'>#{alert}</div>" if alert %>
      </div>
    
      <%= yield %>
    
    </div>
    <!-- end .grid_12 -->
  </div>
  <!-- end .container_12 -->
</body>
</html>
